{
  "name": "cactive-bin",
  "version": "1.0.0",
  "description": "Cactive's Compiled NPM Modules for global usage",
  "main": "./src/index.ts",
  "bin": "./dist/index.js",
  "scripts": {
    "build": "tsc && pkg -C GZip --ouput cactive-bin ."
  },
  "pkg": {
    "assets": [
      "dist/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-macos-x64",
      "node18-linux-x64"
    ]
  },
  "repository": "https://github.com/CactiveNetwork/cactive-rewrites/bin",
  "author": "Cactive",
  "license": "CactiveNetwork Licence Version 1.0",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "pkg": "^5.8.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "phin": "^3.7.0",
    "ts-command-line-args": "^2.3.1"
  }
}
